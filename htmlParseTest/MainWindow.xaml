        <Button x:Name="button4" Content="Dosyalara yaz" HorizontalAlignment="Left" Margin="10,228,0,0" VerticalAlignment="Top" Width="160" Click="button4_Click"/>
        <Button x:Name="button5" Content="Öğretmen id tablosu seç" HorizontalAlignment="Left" Margin="10,110,0,0" VerticalAlignment="Top" Width="160" Click="button5_Click"/>
